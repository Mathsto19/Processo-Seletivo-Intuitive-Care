    {
      "name": "Operadoras",
      "item": [
        {
          "name": "Listar operadoras (paginado + busca)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "url": "{{baseUrl}}/api/operadoras?page=1&limit=10&busca=",
            "description": "Retorna uma lista paginada de operadoras.\nParâmetros:\n- page (int): página (1..n)\n- limit (int): itens por página\n- busca (str): termo para filtrar por Razão Social ou CNPJ (aceita pontuação)."
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status 200', function () { pm.response.to.have.status(200); });",
                  "try {",
                  "  const j = pm.response.json();",
                  "  if (j?.data?.length) {",
                  "    const v = String(j.data[0].cnpj);",
                  "    pm.collectionVariables.set('cnpj', v);",
                  "    pm.environment.set('cnpj', v);",
                  "  }",
                  "} catch (e) {}"
                ]
              }
            }
          ],
          "response": [
            {
              "name": "200 - OK (exemplo)",
              "originalRequest": {
                "method": "GET",
                "header": [
                  {
                    "key": "Accept",
                    "value": "application/json"
                  }
                ],
                "url": "{{baseUrl}}/api/operadoras?page=1&limit=10&busca="
              },
              "status": "OK",
              "code": 200,
              "_postman_previewlanguage": "json",
